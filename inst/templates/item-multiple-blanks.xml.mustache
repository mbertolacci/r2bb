<item title="{{title}}" maxattempts="0">
  <itemmetadata>
    <bbmd_asi_object_id>{{id}}</bbmd_asi_object_id>
    <bbmd_asitype>Item</bbmd_asitype>
    <bbmd_assessmenttype>Pool</bbmd_assessmenttype>
    <bbmd_sectiontype>Subsection</bbmd_sectiontype>
    <bbmd_questiontype>Fill in the Blank Plus</bbmd_questiontype>
    <bbmd_is_from_cartridge>false</bbmd_is_from_cartridge>
    <bbmd_is_disabled>false</bbmd_is_disabled>
    <bbmd_negative_points_ind>N</bbmd_negative_points_ind>
    <bbmd_canvas_fullcrdt_ind>false</bbmd_canvas_fullcrdt_ind>
    <bbmd_all_fullcredit_ind>false</bbmd_all_fullcredit_ind>
    <bbmd_numbertype>none</bbmd_numbertype>
    <bbmd_partialcredit>{{partial_credit}}</bbmd_partialcredit>
    <bbmd_orientationtype>vertical</bbmd_orientationtype>
    <bbmd_is_extracredit>false</bbmd_is_extracredit>
    <bbmd_ai_state>No</bbmd_ai_state>
    <qmd_absolutescore_max>{{max_score}}</qmd_absolutescore_max>
    <qmd_weighting>0</qmd_weighting>
    <qmd_instructornotes>{{{instructor_notes}}}</qmd_instructornotes>
  </itemmetadata>
  <presentation>
    <flow class="Block">
      <flow class="QUESTION_BLOCK">
        <flow class="FORMATTED_TEXT_BLOCK">
          <material>
            <mat_extension>
              <mat_formattedtext type="HTML"><![CDATA[{{{question_text}}}]]></mat_formattedtext>
            </mat_extension>
          </material>
        </flow>
      </flow>
      <flow class="RESPONSE_BLOCK">
        {{#responses}}
          <response_str ident="{{name}}" rcardinality="Single" rtiming="No">
            <render_fib charset="us-ascii" encoding="UTF_8" rows="0" columns="0" maxchars="0"
              prompt="Box" fibtype="String" minnumber="0" maxnumber="0" />
          </response_str>
        {{/responses}}
      </flow>
    </flow>
  </presentation>
  <resprocessing scoremodel="SumOfScores">
    <outcomes>
      <decvar varname="SCORE" vartype="Decimal" defaultval="0" minvalue="0" maxvalue="{{max_score}}" />
    </outcomes>
    <respcondition title="correct">
      <conditionvar>
        <and>
          {{#responses}}
            <or>
              {{#answers}}
                {{#is_exact}}
                  <varequal respident="{{name}}" case="{{case}}">{{text}}</varequal>
                {{/is_exact}}
                {{#is_contains}}
                  <varsubset respident="{{name}}" case="{{case}}" setmatch="Contains">{{text}}</varsubset>
                {{/is_contains}}
                {{#is_matches}}
                  <varsubset respident="{{name}}" case="{{case}}" setmatch="Matches">{{text}}</varsubset>
                {{/is_matches}}
              {{/answers}}
            </or>
          {{/responses}}
        </and>
      </conditionvar>
      <setvar variablename="SCORE" action="Set">SCORE.max</setvar>
      <displayfeedback linkrefid="correct" feedbacktype="Response" />
    </respcondition>
    <respcondition title="incorrect">
      <conditionvar>
        <other />
      </conditionvar>
      <setvar variablename="SCORE" action="Set">0</setvar>
      <displayfeedback linkrefid="incorrect" feedbacktype="Response" />
    </respcondition>
  </resprocessing>
  <itemfeedback ident="correct" view="All">
    <flow_mat class="Block">
      <flow_mat class="FORMATTED_TEXT_BLOCK">
        <material>
          <mat_extension>
            <mat_formattedtext type="HTML"><![CDATA[{{{positive_feedback}}}]]></mat_formattedtext>
          </mat_extension>
        </material>
      </flow_mat>
    </flow_mat>
  </itemfeedback>
  <itemfeedback ident="incorrect" view="All">
    <flow_mat class="Block">
      <flow_mat class="FORMATTED_TEXT_BLOCK">
        <material>
          <mat_extension>
            <mat_formattedtext type="HTML"><![CDATA[{{{negative_feedback}}}]]></mat_formattedtext>
          </mat_extension>
        </material>
      </flow_mat>
    </flow_mat>
  </itemfeedback>
</item>
